<?xml version="1.0" encoding="UTF-8" ?>
<data>
    <!-- 产品名称 -->
    <product>
        <item name="app_name"/>
    </product>

    <!-- 渠道配置 -->
    <channel>
        <!-- mark是渠道标记, name是渠道名称 -->
        <item mark="Google" name="GooglePlay"/>
        <item mark="360" name="_360app"/>
        <item mark="xioami" name="xiaomiapp"/>
        <item mark="meizu" name="meizustore"/>
    </channel>

    <!-- 打包人员 -->
    <person>
        <!-- mark是人员标记, name是打包人名字 -->
        <item mark="developer" name="developer"/>
    </person>

    <!-- 当前apk中的渠道号 -->
    <currentChannel>
        <item name="currentChannelId"/>
    </currentChannel>

    <!-- 修改AndroidManifest.xml配置 -->
    <manifest>
        <!-- 修改 meta_data 标签 -->
        <!--<meta_data name="meta_data" value="meta_data"/>-->
    </manifest>

    <!-- 修改res下的资源文件 -->
    <resource>
        <!-- 修改strings.xml内容 -->
        <strings>
            <!--<string name="string" value="string" />-->
        </strings>

        <!-- 修改bools.xml内容 -->
        <bools>
            <!--<bool name="bool" value="bool" />-->
        </bools>
    </resource>
</data>